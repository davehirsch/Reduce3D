opies the BBWindow class.//// Inputs:	inOriginal	- The object to copy//		// Outputs:	none	////**************************************************************************************BBWindow::BBWindow(const BBWindow& inOriginal)	: PP::LWindow(inOriginal){}//**************************************************************************************// Function:	Destructor//// Description: Destroys the BBWindow class.//// Inputs:	none//		// Outputs:	none	////**************************************************************************************BBWindow::~BBWindow(){}// ---------------------------------------------------------------------------------//		¥ FinishCreateSelf// ---------------------------------------------------------------------------------voidBBWindow::FinishCreateSelf(){	mTheApp = (CReduceApp *) PP::LCommander::GetTopCommander();	mTheScroller = (PP::LActiveScroller *) FindPaneByID(k_Scroller);	mTheBoundingBox = mTheApp->GetBBox();		Show();	Activate();	mTheScroller->AdjustScrollBars();}